{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jasmine": true
  },
  "extends": "standard",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "no-else-return": "error",
    "no-multiple-empty-lines": "off",
    "no-unused-vars": ["error", { "varsIgnorePattern": "(h|es6)" }]
  }
}
